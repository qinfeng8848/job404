<div class="col-md-12">
  <div class="job">
    <%= link_to("Edit", edit_admin_job_path(@job), class: "btn btn-primary pull-right")%>
  </div>
  <h2><%= @job.title %></h2>
  <p>职位描述:<%= @job.description %></p>
  <p>工资下限:<%= @job.wage_lower_bound %></p>
  <p>工资上限:<%= @job.wage_upper_bound %></p>
  <p>联系方式:<%= @job.contact_email %></p>
</div>
